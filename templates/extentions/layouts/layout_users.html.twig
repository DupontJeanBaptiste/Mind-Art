<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <link rel="icon" type="image/png" href="{{ asset('build/images/favicon.png') }}"/>
        <title>
            {% block title %}{% endblock %}
        </title>
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
        
    </head>
    <body>
        <header>
            {% block header %}{% include 'extentions/navbars/navbar_buyer.html.twig' %}{% endblock %}
        </header>
        {% block body %}{% endblock %}
        <aside>
            {% block aside %}{% endblock %}
        </aside>
    </body>
     {% block javascripts %}
         {% include 'extentions/script/alert.html.twig' %}
         {% include 'extentions/script/likelist.html.twig' %}

            {{ encore_entry_script_tags('app') }}

        {% endblock %}
</html>
